{
  "name": "libpg-query-monorepo",
  "private": true,
  "description": "PostgreSQL query parser monorepo",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "pnpm --filter libpg-query build",
    "test": "pnpm --filter libpg-query test",
    "clean": "pnpm --filter libpg-query clean",
    "build:v17": "pnpm --filter @libpg-query/v17 build",
    "test:v17": "pnpm --filter @libpg-query/v17 test",
    "clean:v17": "pnpm --filter @libpg-query/v17 clean",
    "build:all": "pnpm -r build",
    "test:all": "pnpm -r test",
    "clean:all": "pnpm -r clean",
    "analyze:sizes": "node scripts/analyze-sizes.js",
    "publish:dry-run": "node scripts/publish-versions.js --dry-run",
    "publish:versions": "node scripts/publish-versions.js",
    "publish:specific": "node scripts/publish-versions.js --versions"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}  